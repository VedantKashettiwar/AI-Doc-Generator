{
	"info": {
		"_postman_id": "ai-doc-generator-collection",
		"name": "AI Documentation Generator",
		"description": "Collection for testing the AI Documentation Generator API",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Health Check",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/api/docs/health",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"api",
						"docs",
						"health"
					]
				}
			},
			"response": []
		},
		{
			"name": "Upload Markdown File",
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "markdown",
							"type": "file",
							"src": []
						}
					]
				},
				"url": {
					"raw": "{{baseUrl}}/api/docs/upload",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"api",
						"docs",
						"upload"
					]
				}
			},
			"response": []
		}
	],
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		}
	],
	"variable": [
		{
			"key": "baseUrl",
			"value": "http://localhost:3000",
			"type": "string"
		}
	]
} 